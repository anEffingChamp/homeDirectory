LP_ENABLE_SSH_COLORS=1
LP_ENABLE_TIME=1
LP_MARK_GIT=''
LP_COLOR_USER_LOGGED="$(colorCode $(whoami))"
LP_COLOR_HOST="$(colorCode $(hostname))"
LP_COLOR_PATH="$(colorCode $(basename $PWD))"
LP_COLOR_NOWRITE=$BOLD_RED

colorDate="$(colorCode $(date +%M))"
LP_PS1_PREFIX="\n$colorDate$(date +%a)"
LP_PS1_POSTFIX="\n ~  "
LP_COLOR_TIME="$colorDate"
